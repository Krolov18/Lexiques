    <derivation name="dé-nominatif" suffix="er" table="adj-er4"/>
  </table>
  <table name="adj-f4" rads="(.*.[:voyelle:]|ser)">
    <like name="nc-f4"/>
  </table>
  <table name="adj-2" rads="..*[^sxz]" except=".e">
