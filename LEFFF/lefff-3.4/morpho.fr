    <derivation name="adj_dénominal_en_-el" suffix="∂el" table="adj-l4"/>
    <derivation name="adj_dénominal_en_-el_variante_nn" suffix="nel" table="adj-l4" rads=".*n"/>
    <derivation name="adj_dénominal_en_-el_variante" suffix="∂∂el" table="adj-l4" rads=".*eur"/>

    <derivation name="adj_dénominal_en_-ial" suffix="∂ial" table="adj-al4"/>
    <derivation name="adj_dénominal_en_-ial_variante" suffix="∂∂ial" table="adj-al4" rads=".*eur"/>

    <derivation name="adj_dénominal_en_-aire" suffix="∂aire" table="adj-2"/>
