  <!-- <table name="tag_unknown" rads=".*" fast="-"> -->
  <!--     <form suffix="" tag="-"/> -->
  <!-- </table> -->
  <!-- INVARIABLE -->
  <table name="inv" rads="..*" fast="-">
    <form suffix="" tag=""/>
  </table>
  <!-- invariable morphologiquement, mais élision du e possible -->
  <table name="inv-e" rads="..*" fast="-">
    <form suffix="e" tag=""/>
    <form suffix="'" tag="e"/>
  </table>
  <!-- ADVERBES -->
  <table name="adv" rads="...*ment">
    <form suffix="" tag=""/>
  </table>
  <!-- ADJECTIFS -->
  <table name="adj-ique2" rads="..*i">
    <form suffix="que" tag="s"/>
    <form suffix="ques" tag="p"/>
    <derivation name="causatif-déadjectival-ifier" suffix="fier" table="v-er:std"/>
    <derivation name="causatif-déadjectival-iser" suffix="ser" table="v-er:std"/>
  </table>
  <table name="adj-blanc" rads="(blan|fran|se)" fast="-">
    <form suffix="c" tag="ms"/>
    <form suffix="cs" tag="mp"/>
    <form suffix="che" tag="fs"/>
    <form suffix="ches" tag="fp"/>
    <derivation name="péjoratif-âtre" suffix="châtre" table="adj-2"/>
    <derivation name="adjectif_nominalisé" suffix="c" table="nc-blanc"/>
    <derivation name="causatif_déadjectival-ir" suffix="chir" table="v-ir2"/>
    <derivation name="nom_déadjectival-eur" suffix="cheur" table="nc-2f"/>
  </table>
  <table name="adj-eur" rads="..*">
    <like name="nc-eur"/>
  </table>
  <table name="adj-teur" rads="..*[dt]">
    <like name="nc-teur"/>
  </table>
  <table name="adj-1" rads="..*">
    <form suffix="" tag=""/>
  </table>
  <table name="adj-1ms" rads="..*" fast="-">
    <form suffix="" tag="ms"/>
  </table>
  <table name="adj-s3" rads="(..*[iu][sz]|..*[:voyelle:]rs|abscons|.*clos|indispos|dispos|ras)">
