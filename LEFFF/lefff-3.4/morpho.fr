    <derivation name="adjectif_nominalisé" suffix="" table="nc-2"/>
    <derivation name="causatif-déadjectival-ifier" suffix="ifier" table="v-er:std"/>
    <derivation name="causatif-déadjectival-iser" suffix="iser" table="v-er:std"/>
    <derivation name="causatif_déadjectival-ir" suffix="ir" table="v-ir2"/>
  </table>
  <table name="adj-4" rads="..*[^sxzce]">
    <like name="nc-4"/>
    <derivation name="dé-nominatif" suffix="" table="adj-4"/>
  </table>
  <table name="adj-x3" rads="..*u">
    <like name="nc-x3"/>
    <derivation name="dé-nominatif" suffix="x" table="adj-x3"/>
  </table>
  <table name="adj-gros3" rads="..*[aio]s">
