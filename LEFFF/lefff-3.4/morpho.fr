  </table>
  <table name="adj-gu4" rads=".*.gu">
    <form suffix="" tag="ms"/>
    <form suffix="ë" tag="fs"/>
    <form suffix="s" tag="mp"/>
    <form suffix="ës" tag="fp"/>
