%start S

S[SEM=<?s(?v)>] -> SN[SEM=?s] SV[SEM=?v]

SN[SEM=?n] -> NP[SEM=?n]
SN[SEM=<?d(?n)>] -> Det[SEM=?d] N[SEM=?n]

SV[SEM=?v] -> Vi[SEM=?v]

NP[SEM=<\P.P(belbo)>] -> 'Belbo'
Det[SEM=<\P Q. exists x. (P(x) & Q(x))>] -> 'un'
N[SEM=<\x.oiseau(x)>] -> 'oiseau'
Vi[SEM=<\x.vole(x)>] -> 'vole'
Vi[SEM=<\x.pleure(x)>] -> 'pleure'
